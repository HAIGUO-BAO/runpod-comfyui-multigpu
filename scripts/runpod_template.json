{
  "name": "ComfyUI Multi-GPU",
  "description": "ComfyUI with automatic GPU optimization",
  "container_image": "runpod/pytorch:1.0.2-cu1281-torch280-ubuntu2404",
  "container_disk": 50,
  "volume_disk": 100,
  "start_command": "/workspace/start.sh",
  "ports": ["8188", "8080", "8888"],
  "environment_variables": [
    {
      "name": "NVIDIA_VISIBLE_DEVICES",
      "value": "all",
      "description": "Make all GPUs visible"
    },
    {
      "name": "PYTHONUNBUFFERED",
      "value": "1",
      "description": "Real-time Python output"
    }
  ],
  "gpu_types": [
    "NVIDIA GeForce RTX 4090",
    "NVIDIA RTX 6000 Ada",
    "NVIDIA GeForce RTX 4080",
    "NVIDIA GeForce RTX 4070"
  ],
  "version": "1.0.0"
}
